{
  "name": "benji-contracts-v2",
  "version": "1.0.0",
  "description": "Benji Bananas Upgraded contracts",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "postinstall": "bash -c 'for cfg in .vscode/settings.json .vscode/extensions.json .vscode/launch.json; do cp -n ${cfg}.default ${cfg} || :; done'",
    "lint:js": "eslint \"**/*.{js,ts}\"",
    "lint:sol": "solhint \"contracts/**/*.sol\"",
    "lint": "run-s lint:js lint:sol",
    "fix:js": "eslint --fix \"**/*.{js,ts}\"",
    "fix:sol": "solhint --fix \"contracts/**/*.sol\"",
    "fix": "run-s fix:js fix:sol",
    "format:js": "prettier --write \"**/*.{ts,js}\"",
    "format:sol": "prettier --write \"**/*.sol\"",
    "format": "run-s format:js format:sol",
    "compile": "hardhat compile --show-stack-traces",
    "test": "hardhat test --show-stack-traces",
    "test-p": "hardhat test --parallel --show-stack-traces",
    "coverage": "cross-env COVERAGE=true hardhat coverage --show-stack-traces",
    "docgen:sol": "hardhat docgen --show-stack-traces",
    "docgen": "run-s docgen:sol",
    "clean:vscode": "rm -rf .vscode/extensions.json .vscode/launch.json .vscode/settings.json || :",
    "clean:compile": "rm -rf cache || :",
    "clean:artifacts:soft": "find artifacts/* | grep -v '^artifacts/build-info\\|^artifacts/contracts' | xargs rm -rf || :",
    "clean:artifacts:hard": "rm -rf artifacts || :",
    "clean:hardhat": "rm -rf hardhat.config.last.json || :",
    "clean:coverage": "rm -rf coverage coverage.json *.coverage || :",
    "clean:doc": "rm -rf docs || :",
    "clean": "run-p clean:compile clean:artifacts:soft clean:hardhat clean:coverage clean:doc",
    "dev:localhost": "hardhat --network localhost deploy --watch",
    "deploy": "hardhat deploy",
    "deploy:localhost": "hardhat deploy --network localhost",
    "deploy:goerli": "hardhat deploy --network goerli",
    "deploy:mainnet": "hardhat deploy --network mainnet",
    "deploy:mumbai": "hardhat deploy --network mumbai",
    "deploy:matic": "hardhat deploy --network matic",
    "verify:goerli": "hardhat --show-stack-traces --network goerli etherscan-verify",
    "verify:mainnet": "hardhat --show-stack-traces --network mainnet etherscan-verify",
    "verify:mumbai": "hardhat --show-stack-traces --network mumbai etherscan-verify",
    "verify:matic": "hardhat --show-stack-traces --network polygon etherscan-verify",
    "run-all": "run-s format fix lint clean clean:artifacts:hard compile docgen test-p coverage deploy",
    "prepack": "run-s clean:artifacts:hard compile clean"
  },
  "devDependencies": {
    "@animoca/ethereum-contract-helpers": "0.0.4",
    "@animoca/ethereum-contracts": "=1.1.1",
    "@animoca/ethereum-contracts-1.0": "npm:@animoca/ethereum-contracts@1.0.0",
    "@animoca/ethereum-contracts-bridging-0.3": "npm:@animoca/ethereum-contracts-bridging@0.3",
    "@animoca/ethereum-migrations": "1.0.10",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.6",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.8",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@openzeppelin/contracts": "=4.8.3",
    "chai": "^4.3.7",
    "cli-progress": "^3.12.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "ethers": "^5.4.7",
    "ethjs-unit": "^0.1.6",
    "glob": "^10.2.3",
    "hardhat": "^2.14.0",
    "hardhat-deploy": "^0.11.29",
    "hardhat-deploy-ethers": "^0.3.0-beta.13",
    "hardhat-gas-reporter": "^1.0.9",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "lodash.mergewith": "^4.6.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "prettier-plugin-solidity": "^1.1.3",
    "solhint": "^3.4.1",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.8.2",
    "solidity-docgen": "^0.6.0-beta.35",
    "web3": "^1.10.0",
    "yesno": "^0.4.0"
  }
}
